{
  "version": "0.5.1",
  "variants": [
    "lts",
    "current"
  ],
  "build": {
    "latest": "current",
    "rootDistro": "debian",
    "architectures": [
      "linux/amd64",
      "linux/arm64"
    ],
    "tags": [
      "node-essential-devcontainer:${VERSION}-${VARIANT}"
    ],
    "variantTags": {
      "lts": [
        "node-essential-devcontainer:${VERSION}-lts"
      ],
      "current": [
        "node-essential-devcontainer:${VERSION}-current"
      ]
    }
  },
  "dependencies": {
    "image": "node:${VARIANT}-bookworm",
    "imageLink": "https://hub.docker.com/_/node",
    "debian": [
      {
        "cgIgnore": false,
        "name": "yarn"
      }
    ],
    "git": {
      "Oh My Zsh!": "/home/node/.oh-my-zsh",
      "nvm": "/usr/local/share/nvm"
    },
    "npm": [
      "eslint",
      "tslint-to-eslint-config",
      "typescript"
    ],
    "languages": {
      "Node.js": {
        "cgIgnore": true,
        "path": "/usr/local"
      }
    },
    "other": {
      "git": { }
    }
  }
}
